<Window x:Class="CriticsClub.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CriticsClub"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="600">
    <Grid>
        <Grid HorizontalAlignment="Left" Height="420" VerticalAlignment="Top" Width="222" Margin="0,1,0,0">
            
            <Grid HorizontalAlignment="Left" Height="59" Margin="2,1,-372,0" VerticalAlignment="Top" Width="592">
                <Label Content="Book Club" HorizontalAlignment="Left" Margin="0,25,0,0" VerticalAlignment="Top" Width="220" FontSize="18" FontWeight="Bold"/>
                
            </Grid>
        </Grid>
        <Frame x:Name="Main" Margin="180,62,0,0" NavigationUIVisibility="Hidden"/>
        <StackPanel HorizontalAlignment="Left" Height="312" Margin="-1,61,0,0" VerticalAlignment="Top" Width="179">
            <Button Content="Join a Club" Click="Button_Click"/>
            <Button Content="Create a Club" Click="Button_Click_1"/>
        </StackPanel>


    </Grid>
</Window>
